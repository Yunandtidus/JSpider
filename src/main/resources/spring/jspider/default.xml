<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:context="http://www.springframework.org/schema/context"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<bean id="defaultResultSearcher" class="com.jspider.metier.resultSearcher.impl.GenericResultSearcher"/>

	<bean id="defaultRequestCreator" class="com.jspider.metier.requestCreator.impl.GenericRequestCreator"/>
		
	<bean id="htmlResponseParser" class="com.jspider.metier.responseParser.impl.GenericHtmlResponseParser"/>
	<bean id="jsonResponseParser" class="com.jspider.metier.responseParser.impl.GenericJsonResponseParser"/>
	
</beans>